{
  "name": "@principia/transformers",
  "version": "0.4.0",
  "description": "Monad transformers",
  "repository": "git://github.com/0x706b/principia",
  "license": "MIT",
  "author": "Peter Krol <peter@peterkrol.com>",
  "exports": {},
  "main": "dist/dist/cjs/index.js",
  "module": "./dist/dist/esm-shake/index.js",
  "types": "dist/index.d.ts",
  "scripts": {
    "autofix": "yarn lint --fix",
    "build": "yarn build:types && yarn build:esm-fix && yarn build:intermediary && yarn build:esm-shake && yarn build:cjs",
    "build:cjs": "babel --config-file ./config/cjs.babelrc.json --extensions '.js' --source-maps true --out-dir dist/dist/cjs dist/dist/.intermediary",
    "build:esm-fix": "ttsc --build ./config/tsconfig.esm-fix.json",
    "build:esm-shake": "babel --config-file ./config/esm.babelrc.json --extensions '.js' --source-maps true --out-dir dist/dist/esm-shake dist/dist/.intermediary",
    "build:intermediary": "ttsc --build ./config/tsconfig.intermediary.json",
    "build:postprocess": "ts-node ../../scripts/rewrite-source-maps.ts && ts-node ../../scripts/package.ts && yarn sort-package-json ./dist/package.json",
    "build:test": "ttsc --build ./config/tsconfig.test.json --verbose",
    "build:traced": "yarn build:traced:esm-fix && yarn build:traced:intermediary && yarn build:traced:esm-shake && yarn build:traced:cjs",
    "build:traced:cjs": "babel --config-file ./config/cjs.babelrc.json --extensions '.js' --source-maps true --out-dir dist/dist-traced/cjs dist/dist-traced/.intermediary",
    "build:traced:esm-fix": "ttsc --build ./config/tsconfig.traced.esm-fix.json",
    "build:traced:esm-shake": "babel --config-file ./config/esm.babelrc.json --extensions '.js' --source-maps true --out-dir dist/dist-traced/esm-shake dist/dist-traced/.intermediary",
    "build:traced:intermediary": "ttsc --build ./config/tsconfig.traced.intermediary.json",
    "build:types": "ttsc --build ./config/tsconfig.types.json --verbose",
    "buildAll": "yarn build && yarn build:traced && yarn build:postprocess",
    "clean": "rimraf dist build tsbuildinfo",
    "clean:test": "rimraf build",
    "generateSubpaths": "ts-node ../../scripts/subpath.ts && sort-package-json && prettier --write package.json",
    "lint": "eslint .",
    "prepublish": "yarn buildAll && rimraf dist/dist/.intermediary && rimraf dist/dist-traced/.intermediary",
    "publish:package": "yarn version --no-git-tag-version && yarn clean && yarn build && yarn publish --no-git-tag-version dist",
    "test": "yarn build:test && jest build/test"
  },
  "dependencies": {
    "@principia/compile": "^0.4.0",
    "@principia/base": "^0.4.0",
    "@principia/io": "^0.4.0",
    "core-js": "3"
  },
  "devDependencies": {
    "@types/benchmark": "^2.1.0",
    "benchmark": "^2.1.4"
  },
  "publishConfig": {
    "access": "public",
    "directory": "dist"
  }
}
